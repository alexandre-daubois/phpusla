{
    "name": "alexandredaubois/phpusla",
    "description": "Get useless statistics from your PHP code.",
    "type": "library",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/alexandredaubois/phpusla/issues"
    },
    "require": {
        "nikic/php-parser": "^4.13",
        "symfony/console": "^6.0",
        "symfony/finder": "^6.0",
        "symfony/property-info": "^6.0"
    },
    "config": {
        "platform": {
            "php": "8.0.2"
        },
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "bin": [
        "phpusla"
    ],
    "autoload": {
        "classmap": [
            "src/*"
        ]
    },
    "authors": [
        {
            "name": "Alexandre Daubois",
            "email": "alex.daubois@gmail.com"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^9"
    },
    "scripts": {
        "test": "vendor/bin/phpunit tests"
    }
}
